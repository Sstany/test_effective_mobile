{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "core",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/core/main.go",
            "env": {
                "SERVER_PORT": ":8080",
                "DATABASE_HOST": "localhost",
                "DATABASE_NAME": "subscriptions_db",

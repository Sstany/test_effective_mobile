{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "core",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/core/main.go",
            "env": {
                "SERVER_PORT": ":8080",
                "DATABASE_CONNECTION_STRING":"postgresql://postgres:password@localhost:5432/subscriptions_db?sslmode=disable",
                "DEBUG": "true"
            },
            "args": [],
            "showLog": true,
            "logOutput": "debugger",
            "trace": "verbose",
            "buildFlags": "-tags=dev",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}